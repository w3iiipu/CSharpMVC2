@model IEnumerable<HelloMVC2.Models.Employee>

@foreach (var item in Model)
{
    //partial reutns MvcHtmlString and can be assigned to a variable for manipulation
    @Html.Partial("_Employee", item)

    //renderPartial returns void and the output is written to the output stream. Better performance since it render directly to output stream
    { Html.RenderPartial("_Employee", item);}
<br/>
}
